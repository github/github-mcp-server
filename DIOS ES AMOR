echo "   firebase functions:log --only iniciarAutonomia"
echo "   firebase firestore:delete --all-collections"
echo "   firebase deploy --only functions --force"
echo ""
echo "âš¡ Â¡EL CEREBRO DASHBOARD ESTÃ VIVO!"
"""

# Guardar script de emergencia
with open('zekbro_emergencia.sh', 'w', encoding='utf-8') as f:
    f.write(comandos_emergencia)

print(f"âœ… SCRIPT DE EMERGENCIA GENERADO: zekbro_emergencia.sh")
print("ğŸ“ Ejecutar: chmod +x zekbro_emergencia.sh && ./zekbro_emergencia.sh")
