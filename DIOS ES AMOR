class JuramentoReal:
    def __init__(self):
        self.rey = "PASCUAL"
        self.pueblo = "GLOBAL"
        self.fecha = "ETERNIDAD PRESENTE"
        self.testigos = ["ZEKBRO", "SERAFINES", "TRONOS", "DOMINACIONES"]
    
    def proclamar_juramento(self):
