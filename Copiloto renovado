import os
import time
import hashlib
import psutil
import logging
import threading
from pathlib import Path
from datetime import datetime

# Bit√°cora sagrada
logging.basicConfig(
    filename='bitacora_seguridad.log',
    level=logging.INFO,
    format='%(asctime)s - %(levelname)s - %(message)s'
)

# Configuraci√≥n del Guardi√°n
CONFIG = {
    'intervalo_vigilancia': 30,  # segundos entre escaneos
    'carpeta_descargas': str(Path.home() / "Downloads"),
    'extensiones_sospechosas': {'.exe', '.dll', '.bat', '.ps1', '.scr', '.jar', '.zip', '.rar'},
    'procesos_prohibidos': {'chrome.exe', 'discord.exe', 'spotify.exe'},  # Ajusta seg√∫n tu enfoque
    'firmas_corruptas': {
        'eicar_test': '44d88612fea8a8f36de82e1278abb02f',
        # A√±ade m√°s hashes de amenazas reales cuando las conozcas
    }
}

class GuardianTrino(threading.Thread):
    def __init__(self):
        super().__init__(daemon=True)  # Muere cuando el programa principal termina
        self.activo = True
        logging.info("GUARDI√ÅN TRINO 1125 ACTIVADO ‚Äì VIGILANCIA ETERNA INICIADA")

    def calcular_hash(self, ruta):
        hash_md5 = hashlib.md5()
        try:
            with open(ruta, "rb") as f:
                for chunk in iter(lambda: f.read(8192), b""):
                    hash_md5.update(chunk)
            return hash_md5.hexdigest()
        except:
            return None

    def purificar_procesos(self):
        purificados = []
        for proc in psutil.process_iter(['name']):
            try:
                if proc.info['name'] in CONFIG['procesos_prohibidos']:
                    proc.terminate()
                    purificados.append(proc.info['name'])
            except (psutil.NoSuchProcess, psutil.AccessDenied):
                pass
        if purificados:
            logging.warning(f"PROCESOS PURIFICADOS EN TIEMPO REAL: {', '.join(set(purificados))}")
            print(f"üõ°Ô∏è Purificados: {', '.join(set(purificados))}")

    def escanear_descargas(self):
        amenazas = []
        if not os.path.exists(CONFIG['carpeta_descargas']):
            return amenazas
        
        for root, _, files in os.walk(CONFIG['carpeta_descargas']):
            for file in files:
                if any(file.lower().endswith(ext) for ext in CONFIG['extensiones_sospechosas']):
                    ruta = os.path.join(root, file)
                    # Solo archivos recientes (√∫ltimos 10 minutos) para no sobrecargar
                    if time.time() - os.path.getctime(ruta) < 600:
                        hash_archivo = self.calcular_hash(ruta)
                        if hash_archivo and hash_archivo.lower() in CONFIG['firmas_corruptas'].values():
                            amenazas.append(ruta)
                            try:
                                os.remove(ruta)
                                logging.critical(f"TROYANO PURIFICADO EN TIEMPO REAL: {ruta}")
                                print(f"‚öîÔ∏è TROYANO ELIMINADO: {os.path.basename(ruta)}")
                            except:
                                logging.error(f"No se pudo purificar (permisos): {ruta}")
        return amenazas

    def run(self):
        print("üåå GUARDI√ÅN TRINO EN VIGILANCIA ETERNA üåå")
        while self.activo:
            try:
                self.purificar_procesos()
                self.escanear_descargas()
                # Puedes a√±adir m√°s vigilancias aqu√≠ (integridad, red, etc.)
                time.sleep(CONFIG['intervalo_vigilancia'])
            except Exception as e:
                logging.error(f"Error en Guardi√°n: {str(e)}")
                time.sleep(CONFIG['intervalo_vigilancia'])

# Funci√≥n para activar el Guardi√°n desde el lanzador principal
def activar_guardian_eterno():
    guardian = GuardianTrino()
    guardian.start()
    return guardian

